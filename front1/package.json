{
  "name": "nextjs-chat-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "test:comprehensive": "node tests/run-comprehensive-tests.js",
    "build:analyze": "ANALYZE=true npm run build",
    "update-aws-sdk": "./scripts/update-aws-sdk.sh",
    "clean": "rm -rf .next out dist",
    "precommit": "npm run lint && npm run type-check && npm run test"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agentcore": "3.890.0",
    "@aws-sdk/credential-providers": "^3.872.0",
    "axios": "^1.6.0",
    "next": "15.4.5",
    "react": "^18",
    "react-dom": "^18",
    "react-oidc-context": "^3.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "15.4.5",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "playwright": "^1.57.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}