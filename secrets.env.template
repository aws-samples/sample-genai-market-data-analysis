# AWS Credentials Template
# Copy this file to secrets.env and fill in your actual values
# NEVER commit secrets.env to version control

# AWS Credentials
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_DEFAULT_REGION=us-east-1

# API Keys
MARKET_API_KEY=your_market_api_key_here

# S3 Configuration
S3_CHART_BUCKET=your_s3_bucket_name_here

# Other secrets
DATABASE_URL=your_database_url_here
JWT_SECRET=your_jwt_secret_here